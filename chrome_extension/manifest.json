{
    "manifest_version": 2,

    "name": "Couch for YouTube",
    "version": "0.1",

    "description": "Automatic playlist generation for your YouTube subscriptions",

    "icons": {
        "16": "icon.png"
    },

    "homepage_url": "http://drewjackson.org/couch",

    "permissions": ["tabs", "*://*.youtube.com/feed/subscriptions/*"],

    "content_scripts": [{
        "matches": ["*://*.youtube.com/feed/subscriptions/*"],
        "js": ["main.js"]
    }],

    "short_name": "Couch"

}